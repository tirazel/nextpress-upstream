{#
/**
* Theme layout to display a UCB Article External URL Field.
*
* This will auto-redirect to the external URL specified.  
* Created by jsparks on 11/17/21
*/
#}

{% for item in items %}
{% set linkURL = item.content['#url']|render|striptags|trim %}
{% set linkTitle = item.content['#title']|render|striptags|trim %}

    {% if linkURL|length > 1 %}
    <script>
        window.location.href = `{{ linkURL }}`;
    </script>
    {% endif %}

{% endfor %}